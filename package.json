{
  "name": "mikro_orm",
  "version": "0.0.1",
  "description": "Projeto para estudar a lib mikro-orm e suas funcionalidades",
  "main": "./src/main.ts",
  "author": "André Luís da Silva",
  "license": "MIT",
  "private": true,
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "scripts": {
    "start:dev": "export $(cat .env | xargs) && ts-node-dev --transpile-only --ignore-watch node_modules,__tests__ -r tsconfig-paths/register ./src/server.ts",
    "premigration:create": "export $(cat .env.test | xargs) && npx mikro-orm migration:create",
    "migration:create": "export $(cat .env | xargs) && npx mikro-orm migration:create",
    "premigration:up": "export $(cat .env.test | xargs) && npx mikro-orm migration:up",
    "migration:up": "export $(cat .env | xargs) && npx mikro-orm migration:up",
    "pretest": "export $(cat .env.test | xargs) && npx mikro-orm migration:up",
    "test": "export $(cat .env.test | xargs) && jest"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.5.9",
    "@nestjs/testing": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.4",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/postgresql": "^4.5.9",
    "@mikro-orm/reflection": "^4.5.9",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^4.5.9",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  }
}
